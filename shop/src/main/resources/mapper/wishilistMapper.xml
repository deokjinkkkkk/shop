<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dj.shop.mapper.WishiListMapper">
	<select id="checkWishiList" resultType="com.dj.shop.vo.AddressVO">
	select *
    FROM wishlist
    where wishilist_Num = #{wishilistNum} AND user_number = #{userNumber}
	</select>

	<select id="getWishiList" resultType="com.dj.shop.vo.AddressVO">
	select *
    FROM wishilist
    where user_number = #{userNumber}
	</select>
	<insert id="addWishiList">
	insert into wishilist(product_num,user_number)
	values(#{productNum},#{userNumber})
	</insert>

	
	<delete id="removeWishiList">
	Delete FROM address
	where wishilist_num = #{wisilistNum}
	
	</delete>
</mapper>